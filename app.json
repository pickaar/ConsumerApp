{
  "expo": {
    "name": "PickaarConsumerApp",
    "slug": "PickaarConsumerApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.pickaar.consumerapp",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pickaar.consumerapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c5cb5c28-66a0-4898-8f92-7fcb5c9da2fb"
      }
    }
  },
  "extra": {
      "EXPO_USER_BASE_URL": "${process.env.EXPO_USER_BASE_URL}",
      "EXPO_BOOKING_BASE_URL": "${process.env.EXPO_BOOKING_BASE_URL}",
      "EXPO_GOOGLE_PLACES_API_KEY": "${process.env.EXPO_GOOGLE_PLACES_API_KEY}",
      "SENTRY_DSN": "${process.env.SENTRY_DSN}"
    }
}
